  <TrackingTable TableSchema="dbo" TableName="Version">
    <PrimaryKey>Pk_VersionId</PrimaryKey>
    <ForeignKeys>
      <ForeignKey TableSchema="dbo" TableName="Specification">Fk_SpecificationId</ForeignKey>
      <ForeignKey TableSchema="dbo" TableName="Release">Fk_ReleaseId</ForeignKey>
    </ForeignKeys>
    <Columns>
      <Column>MajorVersion</Column>
      <Column>TechnicalVersion</Column>
      <Column>EditorialVersion</Column>
      <Column>AchievedDate</Column>
      <Column>ExpertProvided</Column>
      <Column>Location</Column>
      <Column>SupressFromSDO_Pub</Column>
      <Column>ForcePublication</Column>
      <Column>DocumentUploaded</Column>
      <Column>DocumentPassedToPub</Column>
      <Column>Multifile</Column>
      <Column>Source</Column>
      <Column>ETSI_WKI_ID</Column>
      <Column>ProvidedBy</Column>
      <Column>ETSI_WKI_Ref</Column>
    </Columns>
  </TrackingTable>
</TrackingTables>