<?xml version="1.0" encoding="utf-8" ?>
<log4net>
  <appender name="UserRightsLogFileAppender" type="log4net.Appender.RollingFileAppender">
    <file value="D:\IISLogs\ImportLogs\"/>
    <datePattern value="yyyy.MM.dd'.DbImport.log.resources'" />
    <rollingStyle value="Date"/>
    <staticLogFileName value="false" />
    <appendToFile value="true"/>
    <maximumFileSize value="10MB"/>
    <maxSizeRollBackups value="5"/>
    <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
    <layout type="log4net.Layout.PatternLayout">
      <conversionPattern value="%message%newline"/>
      <locationInfo value="true" />
    </layout>
  </appender>
  <logger name="DbImportLogger">
    <level value="ALL"/>
    <appender-ref ref="UserRightsLogFileAppender"/>
  </logger>
</log4net>

